    "format": "prettier \"**/*.{js,ts,css,scss,md,json,liquid}\" --write"
  },
  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^1.10.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-liquid": "^0.0.1-security",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.20"
  }
}
