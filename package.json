{
  "name": "whiffed-theme",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n vite,shopify -c blue,green \"vite build --watch\" \"shopify theme dev\"",
    "dev:vite": "vite build --watch",
    "dev:shopify": "shopify theme dev",
    "build": "vite build",
