<product-info
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }}"
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="true"
  data-url="{{ product.url }}"
  {% if section.settings.image_zoom == 'hover' %}
    data-zoom-on-hover
  {% endif %}
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

  {% comment %} Swiper CSS {% endcomment %}
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">

  {% unless product.has_only_default_variant %}
    {{ 'component-product-variant-picker.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch-input.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch.css' | asset_url | stylesheet_tag }}
  {% endunless %}
  {%- if product.quantity_price_breaks_configured? -%}
    {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
  {%- endif -%}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {%- if product.quantity_price_breaks_configured? -%}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}

  {% comment %} start block  {% endcomment %}
  <div class="container mx-auto px-4 py-8">
    {% comment %} Breadcrumb {% endcomment %}
    <div class="text-sm text-muted-foreground mb-6">
      <span class="hover:text-primary cursor-pointer">
        <a href="{{ routes.root_url }}">Shop</a>
      </span>
      <span class="mx-2">/</span>
      {% if collection %}
        <span class="hover:text-primary cursor-pointer">
          <a href="{{ collection.url }}">{{ collection.title }}</a>
        </span>
        <span class="mx-2">/</span>
      {% endif %}
      <span class="text-foreground">{{ product.title }}</span>
    </div>

    <div class="grid lg:grid-cols-2 gap-12 mb-16">
      <div class="space-y-4">
        {% comment %} Main Product Image Swiper {% endcomment %}
        <div class="relative group">
          <div class="swiper product-main-swiper aspect-square rounded-lg overflow-hidden bg-card border border-border">
            <div class="swiper-wrapper">
              {% if product.media.size > 0 %}
                {% for media in product.media limit: 10 %}
                  <div class="swiper-slide">
                    {% if media.media_type == 'image' %}
                      <img
                        alt="{{ media.alt | escape }}"
                        loading="lazy"
                        decoding="async"
                        class="object-cover w-full h-full"
                        src="{{ media | image_url: width: 800 }}"
                      >
                    {% elsif media.media_type == 'video' %}
                      <video
                        class="object-cover w-full h-full"
                        controls
                        preload="metadata"
                      >
                        <source src="{{ media.sources[1].url }}" type="{{ media.sources[1].mime_type }}">
                        Your browser does not support the video tag.
                      </video>
                    {% endif %}
                  </div>
                {% endfor %}
              {% else %}
                <div class="swiper-slide">
                  <div class="w-full h-full bg-gray-100 flex items-center justify-center">
                    <span class="text-gray-400">No image available</span>
                  </div>
                </div>
              {% endif %}
            </div>

            {% comment %} Navigation buttons {% endcomment %}
            <div class="swiper-button-prev !left-4 !w-12 !h-12 !mt-0 !top-1/2 !-translate-y-1/2 !rounded-full !bg-background/80 backdrop-blur-sm !border !border-border !opacity-0 group-hover:!opacity-100 !transition-opacity hover:!bg-primary hover:!border-primary hover:!text-primary-foreground !text-foreground after:!hidden">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="24"
                height="24"
                viewBox="0 0 24 24"
                fill="none"
                stroke="currentColor"
                stroke-width="2"
                stroke-linecap="round"
                stroke-linejoin="round"
                class="lucide lucide-chevron-left h-6 w-6"
              >
                <path d="m15 18-6-6 6-6"></path>
              </svg>
            </div>
            <div class="swiper-button-next !right-4 !w-12 !h-12 !mt-0 !top-1/2 !-translate-y-1/2 !rounded-full !bg-background/80 backdrop-blur-sm !border !border-border !opacity-0 group-hover:!opacity-100 !transition-opacity hover:!bg-primary hover:!border-primary hover:!text-primary-foreground !text-foreground after:!hidden">
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="24"
                height="24"
                viewBox="0 0 24 24"
                fill="none"
                stroke="currentColor"
                stroke-width="2"
                stroke-linecap="round"
                stroke-linejoin="round"
                class="lucide lucide-chevron-right h-6 w-6"
              >
                <path d="m9 18 6-6-6-6"></path>
              </svg>
            </div>

            {% comment %} Product badges {% endcomment %}
            <div class="absolute top-4 left-4 flex flex-col gap-2">
              {% if product.compare_at_price > product.price %}
                <span class="inline-flex items-center justify-center rounded-md border text-xs w-fit whitespace-nowrap shrink-0 bg-primary text-primary-foreground px-3 py-1 font-bold">
                  ON SALE
                </span>
              {% endif %}
              {% if product.tags contains 'bestseller' %}
                <span class="inline-flex items-center justify-center rounded-md border text-xs w-fit whitespace-nowrap shrink-0 bg-accent text-accent-foreground px-3 py-1 font-bold">
                  BESTSELLER
                </span>
              {% endif %}
              {% if product.tags contains 'new' %}
                <span class="inline-flex items-center justify-center rounded-md border text-xs w-fit whitespace-nowrap shrink-0 bg-green-600 text-white px-3 py-1 font-bold">
                  NEW
                </span>
              {% endif %}
            </div>
          </div>
        </div>
        {% comment %} Thumbnail Swiper {% endcomment %}
        {% if product.media.size > 1 %}
          <div class="swiper product-thumbs-swiper">
            <div class="swiper-wrapper">
              {% for media in product.media limit: 10 %}
                <div class="swiper-slide">
                  <button class="relative aspect-square rounded-lg overflow-hidden border-2 transition-all border-border hover:border-primary/50 swiper-slide-thumb-active:border-primary w-full">
                    {% if media.media_type == 'image' %}
                      <img
                        alt="{{ media.alt | escape }}"
                        loading="lazy"
                        decoding="async"
                        class="object-cover w-full h-full"
                        src="{{ media | image_url: width: 200 }}"
                      >
                    {% elsif media.media_type == 'video' %}
                      <div class="relative w-full h-full">
                        <img
                          alt="{{ media.alt | escape }}"
                          loading="lazy"
                          decoding="async"
                          class="object-cover w-full h-full"
                          src="{{ media.preview_image | image_url: width: 200 }}"
                        >
                        <div class="absolute inset-0 flex items-center justify-center bg-black/20">
                          <svg class="w-8 h-8 text-white" fill="currentColor" viewBox="0 0 24 24">
                            <path d="M8 5v14l11-7z"/>
                          </svg>
                        </div>
                      </div>
                    {% endif %}
                  </button>
                </div>
              {% endfor %}
            </div>
          </div>
        {% endif %}
        <div class="grid grid-cols-3 gap-4 pt-4">
          <div
            data-slot="card"
            class="text-card-foreground flex flex-col gap-6 rounded-xl border shadow-sm p-4 text-center border-border bg-card"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="24"
              height="24"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="lucide lucide-truck h-6 w-6 mx-auto mb-2 text-primary"
            >
              <path d="M14 18V6a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2v11a1 1 0 0 0 1 1h2"></path><path d="M15 18H9"></path><path d="M19 18h2a1 1 0 0 0 1-1v-3.65a1 1 0 0 0-.22-.624l-3.48-4.35A1 1 0 0 0 17.52 8H14"></path><circle cx="17" cy="18" r="2"></circle><circle cx="7" cy="18" r="2"></circle>
            </svg>
            <p class="text-xs font-bold text-foreground">FREE SHIPPING</p>
            <p class="text-xs text-muted-foreground">On orders $50+</p>
          </div>
          <div
            data-slot="card"
            class="text-card-foreground flex flex-col gap-6 rounded-xl border shadow-sm p-4 text-center border-border bg-card"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="24"
              height="24"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="lucide lucide-shield h-6 w-6 mx-auto mb-2 text-primary"
            >
              <path d="M20 13c0 5-3.5 7.5-7.66 8.95a1 1 0 0 1-.67-.01C7.5 20.5 4 18 4 13V6a1 1 0 0 1 1-1c2 0 4.5-1.2 6.24-2.72a1.17 1.17 0 0 1 1.52 0C14.51 3.81 17 5 19 5a1 1 0 0 1 1 1z"></path>
            </svg>
            <p class="text-xs font-bold text-foreground">GUARANTEED</p>
            <p class="text-xs text-muted-foreground">100% Authentic</p>
          </div>
          <div
            data-slot="card"
            class="text-card-foreground flex flex-col gap-6 rounded-xl border shadow-sm p-4 text-center border-border bg-card"
          >
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="24"
              height="24"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="lucide lucide-rotate-ccw h-6 w-6 mx-auto mb-2 text-primary"
            >
              <path d="M3 12a9 9 0 1 0 9-9 9.75 9.75 0 0 0-6.74 2.74L3 8"></path><path d="M3 3v5h5"></path>
            </svg>
            <p class="text-xs font-bold text-foreground">30-DAY RETURNS</p>
            <p class="text-xs text-muted-foreground">Money back</p>
          </div>
        </div>
      </div>
      <div class="space-y-6">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when '@app' -%}
              {% render block %}
            {%- when 'custom_title' -%}
              {% render 'product-header-custom',
                product: product,
                block: block,
                show_vendor: block.settings.show_vendor,
                show_rating: block.settings.show_rating,
                show_actions: block.settings.show_actions
              %}
            {%- when 'custom_price' -%}
              <div class="flex items-baseline gap-3 mb-4" {{ block.shopify_attributes }}>
                {% if product.compare_at_price > product.price %}
                  <span class="text-4xl font-bold text-primary">{{ product.price | money }}</span>
                  <span class="text-2xl text-muted-foreground line-through">
                    {{- product.compare_at_price | money -}}
                  </span>
                {% else %}
                  <span class="text-4xl font-bold text-primary">{{ product.price | money }}</span>
                {% endif %}
              </div>
            {%- when 'custom_desc' -%}
              {% render 'product-description-custom',
                product: product,
                block: block,
                show_full_description: block.settings.show_full_description
              %}
            {%- when 'custom_variant_picker' -%}
              {% render 'product-variant-selector-custom', product: product, block: block %}
            {%- when 'custom_purchase_options' -%}
              {% render 'product-purchase-options-custom',
                product: product,
                block: block,
                show_subscription: block.settings.show_subscription
              %}
            {%- when 'custom_buy_buttons' -%}
              {% render 'product-form-custom', product: product, block: block, section: section %}
            {%- when 'custom_scent_profile' -%}
              {% render 'product-scent-profile-custom', product: product, block: block %}
            {%- else -%}
              {%- render block -%}
          {%- endcase -%}
        {%- endfor -%}

        {% comment %} Scent profile moved to custom block - add 'custom_scent_profile' block in theme customizer {% endcomment %}
        {% if product.variants.size > 1 %}
          <div>
            <label class="block text-sm font-bold text-foreground mb-3">SELECT VARIANT</label>
            <div class="grid grid-cols-2 md:grid-cols-4 gap-3">
              {% for variant in product.variants %}
                <button
                  class="variant-option p-4 rounded-lg border-2 transition-all text-center border-border hover:border-primary/50 {% if variant == product.selected_or_first_available_variant %}border-primary bg-primary/10{% endif %}"
                  data-variant-id="{{ variant.id }}"
                  data-variant-price="{{ variant.price | money }}"
                >
                  <p class="font-bold text-foreground">{{ variant.title }}</p>
                  <p class="text-xs text-muted-foreground">{{ variant.price | money }}</p>
                </button>
              {% endfor %}
            </div>
          </div>
        {% endif %}
        <div class="space-y-3">
          <div class="w-full p-4 rounded-lg border-2 border-primary bg-primary/10">
            <div class="flex items-center justify-between">
              <div>
                <p class="font-bold text-foreground">One-time purchase</p>
                <p class="text-sm text-muted-foreground" id="selected-price">
                  {{ product.selected_or_first_available_variant.price | money }}
                </p>
              </div>
              <div class="h-5 w-5 rounded-full border-2 border-primary bg-primary">
                <div class="h-full w-full rounded-full bg-background m-0.5"></div>
              </div>
            </div>
          </div>

          {% comment %} Subscription option - can be enabled via settings {% endcomment %}
          {% if section.settings.show_subscription_option %}
            <button class="w-full p-4 rounded-lg border-2 transition-all text-left relative overflow-hidden border-border hover:border-primary/50">
              <span class="absolute top-2 right-2 bg-primary text-primary-foreground text-xs px-2 py-0.5 rounded-md font-medium">
                SAVE 10%
              </span>
              <div class="flex items-center justify-between">
                <div>
                  <p class="font-bold text-foreground">Subscribe &amp; Save</p>
                  <p class="text-sm text-primary font-bold" id="subscription-price">
                    {{ product.selected_or_first_available_variant.price | times: 0.9 | money }} every month
                  </p>
                </div>
                <div class="h-5 w-5 rounded-full border-2 border-border"></div>
              </div>
            </button>
          {% endif %}
        </div>
        {%- render 'buy-buttons',
          block: block,
          product: product,
          product_form_id: product_form_id,
          section_id: section.id,
          show_pickup_availability: true
        -%}
        <p class="text-sm text-muted-foreground text-center">
          Or 4 interest-free payments of $8.75 with <span class="font-bold text-foreground">Afterpay</span>
        </p>
      </div>
    </div>
    <div dir="ltr" data-orientation="horizontal" data-slot="tabs" class="flex flex-col gap-2 mb-16">
      <div
        role="tablist"
        aria-orientation="horizontal"
        data-slot="tabs-list"
        class="text-muted-foreground inline-flex items-center w-full justify-start border-b border-border rounded-none bg-transparent h-auto p-0"
        tabindex="0"
        data-orientation="horizontal"
        style="outline: none;"
      >
        <button
          type="button"
          role="tab"
          aria-selected="true"
          aria-controls="radix-_r_0_-content-details"
          data-state="active"
          id="radix-_r_0_-trigger-details"
          data-slot="tabs-trigger"
          class="dark:data-[state=active]:text-foreground focus-visible:border-ring focus-visible:ring-ring/50 focus-visible:outline-ring dark:data-[state=active]:border-input dark:data-[state=active]:bg-input/30 text-foreground dark:text-muted-foreground inline-flex h-[calc(100%-1px)] flex-1 items-center justify-center gap-1.5 border text-sm whitespace-nowrap transition-[color,box-shadow] focus-visible:ring-[3px] focus-visible:outline-1 disabled:pointer-events-none disabled:opacity-50 data-[state=active]:shadow-sm [&amp;_svg]:pointer-events-none [&amp;_svg]:shrink-0 [&amp;_svg:not([class*='size-'])]:size-4 rounded-none border-b-2 border-transparent data-[state=active]:border-primary data-[state=active]:bg-transparent px-6 py-4 font-bold"
          tabindex="-1"
          data-orientation="horizontal"
          data-radix-collection-item=""
        >
          DETAILS</button
        ><button
          type="button"
          role="tab"
          aria-selected="false"
          aria-controls="radix-_r_0_-content-howto"
          data-state="inactive"
          id="radix-_r_0_-trigger-howto"
          data-slot="tabs-trigger"
          class="dark:data-[state=active]:text-foreground focus-visible:border-ring focus-visible:ring-ring/50 focus-visible:outline-ring dark:data-[state=active]:border-input dark:data-[state=active]:bg-input/30 text-foreground dark:text-muted-foreground inline-flex h-[calc(100%-1px)] flex-1 items-center justify-center gap-1.5 border text-sm whitespace-nowrap transition-[color,box-shadow] focus-visible:ring-[3px] focus-visible:outline-1 disabled:pointer-events-none disabled:opacity-50 data-[state=active]:shadow-sm [&amp;_svg]:pointer-events-none [&amp;_svg]:shrink-0 [&amp;_svg:not([class*='size-'])]:size-4 rounded-none border-b-2 border-transparent data-[state=active]:border-primary data-[state=active]:bg-transparent px-6 py-4 font-bold"
          tabindex="-1"
          data-orientation="horizontal"
          data-radix-collection-item=""
        >
          HOW TO USE</button
        ><button
          type="button"
          role="tab"
          aria-selected="false"
          aria-controls="radix-_r_0_-content-ingredients"
          data-state="inactive"
          id="radix-_r_0_-trigger-ingredients"
          data-slot="tabs-trigger"
          class="dark:data-[state=active]:text-foreground focus-visible:border-ring focus-visible:ring-ring/50 focus-visible:outline-ring dark:data-[state=active]:border-input dark:data-[state=active]:bg-input/30 text-foreground dark:text-muted-foreground inline-flex h-[calc(100%-1px)] flex-1 items-center justify-center gap-1.5 border text-sm whitespace-nowrap transition-[color,box-shadow] focus-visible:ring-[3px] focus-visible:outline-1 disabled:pointer-events-none disabled:opacity-50 data-[state=active]:shadow-sm [&amp;_svg]:pointer-events-none [&amp;_svg]:shrink-0 [&amp;_svg:not([class*='size-'])]:size-4 rounded-none border-b-2 border-transparent data-[state=active]:border-primary data-[state=active]:bg-transparent px-6 py-4 font-bold"
          tabindex="-1"
          data-orientation="horizontal"
          data-radix-collection-item=""
        >
          INGREDIENTS</button
        ><button
          type="button"
          role="tab"
          aria-selected="false"
          aria-controls="radix-_r_0_-content-reviews"
          data-state="inactive"
          id="radix-_r_0_-trigger-reviews"
          data-slot="tabs-trigger"
          class="dark:data-[state=active]:text-foreground focus-visible:border-ring focus-visible:ring-ring/50 focus-visible:outline-ring dark:data-[state=active]:border-input dark:data-[state=active]:bg-input/30 text-foreground dark:text-muted-foreground inline-flex h-[calc(100%-1px)] flex-1 items-center justify-center gap-1.5 border text-sm whitespace-nowrap transition-[color,box-shadow] focus-visible:ring-[3px] focus-visible:outline-1 disabled:pointer-events-none disabled:opacity-50 data-[state=active]:shadow-sm [&amp;_svg]:pointer-events-none [&amp;_svg]:shrink-0 [&amp;_svg:not([class*='size-'])]:size-4 rounded-none border-b-2 border-transparent data-[state=active]:border-primary data-[state=active]:bg-transparent px-6 py-4 font-bold"
          tabindex="-1"
          data-orientation="horizontal"
          data-radix-collection-item=""
        >
          REVIEWS (61)
        </button>
      </div>
      <div
        data-state="active"
        data-orientation="horizontal"
        role="tabpanel"
        aria-labelledby="radix-_r_0_-trigger-details"
        id="radix-_r_0_-content-details"
        tabindex="0"
        data-slot="tabs-content"
        class="flex-1 outline-none py-8"
        style="animation-duration: 0s;"
      >
        <div class="max-w-3xl space-y-4 text-muted-foreground leading-relaxed">
          <p>
            <strong class="text-foreground">Black</strong> is our boldest, most unapologetic scent. Inspired by YSL
            Black Opium, this isn't your grandma's perfume oil—it's a statement.
          </p>
          <p>
            The opening hits with spicy cardamom and pink pepper that wakes you up, then mellows into a floral heart of
            iris and violet that's somehow both delicate and intense. The base? Pure addiction: creamy vanilla, warm
            tonka bean, and earthy patchouli that lingers for hours.
          </p>
          <p>
            <strong class="text-foreground">Perfect for:</strong> Night outs, date nights, or any time you want to leave
            an impression. This scent doesn't whisper—it announces.
          </p>
          <p><strong class="text-foreground">Longevity:</strong> 6-8 hours on skin, even longer on clothes</p>
          <p><strong class="text-foreground">Sillage:</strong> Moderate to heavy—people will notice</p>
        </div>
      </div>
      <div
        data-state="inactive"
        data-orientation="horizontal"
        role="tabpanel"
        aria-labelledby="radix-_r_0_-trigger-howto"
        hidden=""
        id="radix-_r_0_-content-howto"
        tabindex="0"
        data-slot="tabs-content"
        class="flex-1 outline-none py-8"
      ></div>
      <div
        data-state="inactive"
        data-orientation="horizontal"
        role="tabpanel"
        aria-labelledby="radix-_r_0_-trigger-ingredients"
        hidden=""
        id="radix-_r_0_-content-ingredients"
        tabindex="0"
        data-slot="tabs-content"
        class="flex-1 outline-none py-8"
      ></div>
      <div
        data-state="inactive"
        data-orientation="horizontal"
        role="tabpanel"
        aria-labelledby="radix-_r_0_-trigger-reviews"
        hidden=""
        id="radix-_r_0_-content-reviews"
        tabindex="0"
        data-slot="tabs-content"
        class="flex-1 outline-none py-8"
      ></div>
    </div>

    {% comment %} App Blocks and Reviews Section {% endcomment %}
    <div class="mt-16">
      {% comment %}
        {% for block in section.blocks %}
          {% case block.type %}
            {% when '@app' %}
              <div class="mb-8" {{ block.shopify_attributes }}>
                {% render block %}
              </div>
            {% when 'reviews' %}
              <div class="mb-8" {{ block.shopify_attributes }}>
                <div class="bg-card border border-border rounded-lg p-6">
                  <h3 class="text-xl font-bold mb-4 flex items-center gap-2">
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="24"
                      height="24"
                      viewBox="0 0 24 24"
                      fill="none"
                      stroke="currentColor"
                      stroke-width="2"
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      class="lucide lucide-star h-5 w-5 text-primary"
                    >
                      <path d="M11.525 2.295a.53.53 0 0 1 .95 0l2.31 4.679a2.123 2.123 0 0 0 1.595 1.16l5.166.756a.53.53 0 0 1 .294.904l-3.736 3.638a2.123 2.123 0 0 0-.611 1.878l.882 5.14a.53.53 0 0 1-.771.56l-4.618-2.428a2.122 2.122 0 0 0-1.973 0L6.396 21.01a.53.53 0 0 1-.77-.56l.881-5.139a2.122 2.122 0 0 0-.611-1.879L2.16 9.795a.53.53 0 0 1 .294-.906l5.165-.755a2.122 2.122 0 0 0 1.597-1.16z"></path>
                    </svg>
                    Customer Reviews
                  </h3>
                  <div class="reviews-placeholder text-center py-8 text-muted-foreground">
                    <p>
                      {{ block.settings.content | default: 'Reviews will appear here when a review app is installed.' }}
                    </p>
                  </div>
                </div>
              </div>
          {% endcase %}
        {% endfor %}
      {% endcomment %}
    </div>
  </div>

  {% comment %} end block  {% endcomment %}
  <div class="container mx-auto px-4 max-w-7xl">
    <div class="bg-white product-layout grid grid-cols-1 {% if product.media.size > 0 %}lg:grid-cols-2{% endif %} gap-8 lg:gap-12 product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }}{% unless product.media.size > 0 %} product--no-media{% endunless %}">
      <div class="product-media-wrapper order-1 lg:order-1">
        {% render 'product-media-gallery', variant_images: variant_images %}
      </div>
      <div class="product-info-wrapper order-2 lg:order-2{% if settings.page_width > 1400 and section.settings.media_size == "small" %} lg:pl-8{% endif %}{% if settings.animations_reveal_on_scroll %} opacity-0 transform translate-y-8 transition-all duration-700 ease-out{% endif %}">
        <section
          id="ProductInfo-{{ section.id }}"
          class="product-info-container space-y-6{% if section.settings.enable_sticky_info %} lg:sticky lg:top-8{% endif %}"
        >
          {%- assign product_form_id = 'product-form-' | append: section.id -%}

          <a href="{{ product.url }}" class="link product__view-details animate-arrow">
            {{ 'products.product.view_full_details' | t }}
            {{- 'icon-arrow.svg' | inline_asset_content -}}
          </a>
        </section>
      </div>
    </div>

    {% render 'product-media-modal', variant_images: variant_images %}

    {% assign popups = section.blocks | where: 'type', 'popup' %}
    {%- for block in popups -%}
      <modal-dialog id="PopupModal-{{ block.id }}" class="product-popup-modal" {{ block.shopify_attributes }}>
        <div
          role="dialog"
          aria-label="{{ block.settings.text }}"
          aria-modal="true"
          class="product-popup-modal__content"
          tabindex="-1"
        >
          <button
            id="ModalClose-{{ block.id }}"
            type="button"
            class="product-popup-modal__toggle"
            aria-label="{{ 'accessibility.close' | t }}"
          >
            {{- 'icon-close.svg' | inline_asset_content -}}
          </button>
          <div class="product-popup-modal__content-info">
            <h1 class="h2">{{ block.settings.page.title | escape }}</h1>
            {{ block.settings.page.content }}
          </div>
        </div>
      </modal-dialog>
    {%- endfor -%}

    {%- if product.media.size > 0 -%}
      <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- if first_3d_model -%}
      <script type="application/json" id="ProductJSON-{{ product.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
      <script src="{{ 'product-model.js' | asset_url }}" defer></script>
    {%- endif -%}

    {%- liquid
      if product.selected_or_first_available_variant.featured_media
        assign seo_media = product.selected_or_first_available_variant.featured_media
      else
        assign seo_media = product.featured_media
      endif
    -%}

    <script type="application/ld+json">
      {{ product | structured_data }}
    </script>
  </div>
</product-info>

